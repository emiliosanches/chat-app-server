    "@types/express-session": "^1.17.0",
    "@types/nodemailer": "^6.4.0",
    "@types/socket.io": "^2.1.11",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "connect-sqlite3": "^0.9.11",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "knex": "^0.21.5",
    "nodemailer": "^6.4.11",
    "socket.io": "^2.3.0",
    "sqlite3": "^5.0.0",
    "ts-node-dev": "^1.0.0-pre.62"
